	},
	"homepage": "https://github.com/theandrewchon/Sip-Phrase-Generator#readme",
	"dependencies": {
		"axios": "^0.20.0",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"if-env": "^1.0.4",
		"lodash": "^4.17.20",
		"mongoose": "^5.10.5"
	},
	"devDependencies": {
		"concurrently": "^5.3.0",
		"nodemon": "^2.0.4"
	}
